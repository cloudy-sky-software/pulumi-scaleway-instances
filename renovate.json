{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "ignoreDeps": ["github.com/mattn/go-ieproxy"],
  "ignorePaths": ["sdk/**", "examples/**"],
  "hostRules": [
    {
      "matchHost": "https://github.com/cloudy-sky-software/pulumi-provider-framework",
      "encrypted": {
        "token": ""
      },
      "hostType": "go"
    },
    {
      "matchHost": "https://api.github.com/repos/cloudy-sky-software/pulumi-provider-framework",
      "encrypted": {
        "token": ""
      },
      "hostType": "github"
    }
  ],
  "customEnvVariables": {
    "GOPRIVATE": "github.com/cloudy-sky-software/*"
  },
  "postUpdateOptions": ["gomodTidy"]
}
